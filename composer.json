{
    "name": "dipeshsukhia/laravel-remove-public-url",
    "description": "Remove public from laravel project and run the project in the root directory",
    "keywords": [
        "laravel",
        "remove public from URL",
        "run laravel in the root directory"
    ],
    "homepage": "https://github.com/dipeshsukhia/laravel-remove-public-url",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Dipesh Sukhia",
            "email": "dipsh.sukhia@gmail.com",
            "role": "Developer"
        },
        {
            "name": "Bhavin Gajjar",
            "email": "gajjarbhavin22@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*|5.8.*|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Dipeshsukhia\\LaravelRemovePublicUrl\\": "src"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Dipeshsukhia\\LaravelRemovePublicUrl\\LaravelRemovePublicUrlServiceProvider"
            ]
        }
    }
}
